- if @designations.empty?
  #container_form
    .notice{:style => "background: none; text-align: center;"}
      No designation
- else
  %table.dataTable.report.weeksheet.nojs
    %thead
      %tr
        %th
          Id
        %th
          Name
        %th
          Location Level
        %th
          User Role
        %th
          Created At
    - @designations.each do |designation|
      %tr
        %td
          = designation.id
        %td
          = designation.name
        %td
          = designation.location_level.name
        %td
          = designation.role_class.to_s.capitalize
        %td
          = designation.created_at